import os
import argparse
import pandas as pd
import numpy as np

#keeping all the values of GDD are higher than zero
def sureData(dada):
    GDD = []
    for i in data:
        if i >= 0:
           GDD.append(i)
    return GDD

#calculating the values of GDD
def calculate_GDD(Data):
    baseT=10
    Data['GDD'] = ((Data['Max Temp (°C)'] + Data['Min Temp (°C)'])/2) -10
    Data['GDD'] = sureData(Data['GDD'])
    return Data
